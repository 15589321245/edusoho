{%  set contact = setting('contact')%}
{% if  contact.enalbed is not empty and contact.enabled == 1  %}
<div class="scrollsidebar side_green" id="hhService" >
  <div class="side_content" style="width: 167px;">
    <div class="side_list">
      <div class="side_title">
        <a title="隐藏" class="close_btn"><span>关闭</span></a></div>
      <div class="side_center">
        <div class="qqserver">
           <div class="online_bar expand" id="onlineSort1">
              <h2><a id="1">QQ联系</a></h2>
              <div class="online_content overz" id="onlineType1">
                {%  if contact.qq1 is not empty %}
                <p>
                <a target="_blank" href="http://wpa.qq.com/msgrd?v=3&uin={{contact.qq1}}&site=edusoho&menu=yes"><img border="0" src="http://wpa.qq.com/pa?p=2:{{contact.qq1}}:51" alt="点击这里给我发消息" title="点击这里给我发消息"/>在线客服</a>
                </p>
                {% endif %}

                 {%  if contact.qq2 is not empty %}
                <p>
                <a target="_blank" href="http://wpa.qq.com/msgrd?v=3&uin={{contact.qq2}}&site=edusoho&menu=yes"><img border="0" src="http://wpa.qq.com/pa?p=2:{{contact.qq2}}:51" alt="点击这里给我发消息" title="点击这里给我发消息"/>在线客服</a>
                </p>
                {% endif %}

                 {%  if contact.qq3 is not empty %}
                <p>
                <a target="_blank" href="http://wpa.qq.com/msgrd?v=3&uin={{contact.qq3}}&site=edusoho&menu=yes"><img border="0" src="http://wpa.qq.com/pa?p=2:{{contact.qq3}}:51" alt="点击这里给我发消息" title="点击这里给我发消息"/>在线客服</a>
                </p>
                {% endif %}
              </div>
          </div>
          
            <div class="online_bar mycollapse" id="onlineSort2">
              <h2><a  id="2">QQ群联系</a></h2>
              <div class="online_content overz" id="onlineType2" style="display:none">  
            {% if contact.qqgroup1 is not empty %}
                <p>
                    <img border="0" src="http://pub.idqqimg.com/wpa/images/group.png" alt="加入ＱＱ群" title="加入ＱＱ群">
                    {{contact.qqgroup1 }}
                </p>
            {% endif %}

            {% if contact.qqgroup2 is not empty %}
                <p>
                    <img border="0" src="http://pub.idqqimg.com/wpa/images/group.png" alt="加入ＱＱ群" title="加入ＱＱ群">
                    {{contact.qqgroup2 }}
                </p>
            {% endif %}

            {% if contact.qqgroup3 is not empty %}
                <p>
                    <img border="0" src="http://pub.idqqimg.com/wpa/images/group.png" alt="加入ＱＱ群" title="加入ＱＱ群">
                    {{contact.qqgroup3 }}
                </p>
            {% endif %}
          </div>
        </div>

          <div class="online_bar mycollapse" id="onlineSort3">
              <h2><a id="3">热线电话</a></h2>
              <div class="online_content overz" id="onlineType3" style="display:none">  
            {% if contact.hotline1 is not empty %}
                <strong>{{contact.hotline1}}</strong>
            {% endif %}

            {% if contact.hotline1 is not empty %}
                <strong>{{contact.hotline2}}</strong>
            {% endif %}

            {% if contact.hotline1 is not empty %}
                <strong>{{contact.hotline3}}</strong>
            {% endif %}
          </div>
        </div>

        <div class="online_bar mycollapse" id="onlineSort4">
              <h2><a id="4">微信</a></h2>
              <div class="online_content overz" id="onlineType4" style="display:none">  
            {% if contact.webchat is not empty %}
                <strong>微信：{{contact.webchat}}</strong>
            {% endif %}
          </div>
        </div>

          <div class="online_bar mycollapse" id="onlineSort5">
              <h2><a id="5">电子邮件</a></h2>
              <div class="online_content overz" id="onlineType5" style="display:none">  
            {% if contact.email is not empty %}
                <strong>邮件地址：{{contact.email}}</strong>
            {% endif %}
          </div>
        </div>
       
         
        </div>
      </div>
      <div class="side_bottom">
      </div>
    </div>
  </div>
  <div class="show_btn" style="float: right; width: 0px;"><span>马上咨询</span></div>
</div>

{% endif %}