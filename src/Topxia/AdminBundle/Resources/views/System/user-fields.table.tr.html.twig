{% for field in fields %}
		<tr id="field-tr-{{ field.id }}">
		 <td>{{field.title}}</td>
		 <td>{{field.fieldName}}</td>
		 <td>{% if field.enabled %}是{% else %}否{% endif %}</td>
		 <td>{{field.seq}}</td>
		 <td>{{field.createdTime|date('Y-m-d H:i:s')}}</td>
		 <td><button type="button" class="btn btn-default btn-sm " >编辑</button>&nbsp;<button type="button" class="btn btn-default btn-sm " >删除</button></td>
		</tr>
	{% else %}
	  <tr><td colspan="20"><div class="empty">暂无自定义字段</div></td></tr>
  {% endfor %}