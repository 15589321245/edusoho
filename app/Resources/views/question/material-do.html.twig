<div class="material">
	<div class="well testpaper-question-stem-material">
		{{ question.stem|fill_question_stem_html|bbCode2Html|raw }}
		{% include 'attachment/widget/list.html.twig' with {targetType:'question.stem', targetId:question.id  } %}
	</div>

	{% for subQuestion in question.subs|default([]) %}
  
    {% set questionTemplate = getQuestionTypeTemplate(subQuestion.type,'do') %}
    {% if questionTemplate %}
      {% include questionTemplate with {question:subQuestion, parentQuestion:question, seq:loop.index} %}
    {% endif %}
	{% endfor %}
</div>