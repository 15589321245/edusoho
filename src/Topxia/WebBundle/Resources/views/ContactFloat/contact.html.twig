<div class="scrollsidebar" id="hhService">
  <div class="side_content">
    <div class="side_list">
      <div class="side_title">
        <a title="隐藏" class="close_btn"><span>关闭</span></a>
        <span class="siede_title_font">{{contact.worktime}}</span>
      </div>
      
        <div class="qqserver">
           {%  if contact.qq1 is not empty or contact.qq2 is not empty or contact.qq3 is not empty %}
           <div class="online_bar expand" id="onlineSort1">
              <h2><a id="1">QQ联系</a></h2>
              <div class="online_content overz" id="onlineType1">
                {%  if contact.qq1 is not empty %}
                <p>
                <a target="_blank" href="http://wpa.qq.com/msgrd?v=3&uin={{contact.qq1}}&site=edusoho&menu=yes" ><img border="0" src="{{asset('/bundles/topxiaweb/img/contact/qq24.png')}}" alt="点击这里给我发消息" title="点击这里给我发消息"/>在线客服</a>
                </p>
                {% endif %}

                 {%  if contact.qq2 is not empty %}
                <p>
                <a target="_blank" href="http://wpa.qq.com/msgrd?v=3&uin={{contact.qq2}}&site=edusoho&menu=yes"><img border="0" src="{{asset('/bundles/topxiaweb/img/contact/qq24.png')}}" alt="点击这里给我发消息" title="点击这里给我发消息"/>在线客服</a>
                </p>
                {% endif %}

                 {%  if contact.qq3 is not empty %}
                <p>
                <a target="_blank" href="http://wpa.qq.com/msgrd?v=3&uin={{contact.qq3}}&site=edusoho&menu=yes"><img border="0" src="{{asset('/bundles/topxiaweb/img/contact/qq24.png')}}" alt="点击这里给我发消息" title="点击这里给我发消息"/>在线客服</a>
                </p>
                {% endif %}
              </div>
          </div>
           {% endif %}

            {% if contact.qqgroup1 is not empty or  contact.qqgroup2 is not empty or  contact.qqgroup3 is not empty%}
            <div class="online_bar mycollapse" id="onlineSort2">
              <h2><a  id="2">QQ群联系</a></h2>
              <div class="online_content overz" id="onlineType2" style="display:none">  
            {% if contact.qqgroup1 is not empty %}
                <p>
                    <img border="0" src="{{asset('/bundles/topxiaweb/img/contact/qqgroup.png')}}" alt="加入ＱＱ群" title="加入ＱＱ群">
                    {{contact.qqgroup1 }}
                </p>
            {% endif %}

            {% if contact.qqgroup2 is not empty %}
                <p>
                    <img border="0" src="{{asset('/bundles/topxiaweb/img/contact/qqgroup.png')}}" alt="加入ＱＱ群" title="加入ＱＱ群">
                    {{contact.qqgroup2 }}
                </p>
            {% endif %}

            {% if contact.qqgroup3 is not empty %}
                <p>
                    <img border="0" src="{{asset('/bundles/topxiaweb/img/contact/qqgroup.png')}}" alt="加入ＱＱ群" title="加入ＱＱ群">
                    {{contact.qqgroup3 }}
                </p>
            {% endif %}
          </div>
        </div>
        {% endif %}

        {% if contact.hotline1 is not empty or contact.hotline2 is not empty  or contact.hotline3 is not empty %}
          <div class="online_bar mycollapse" id="onlineSort3">
              <h2><a id="3">热线电话</a></h2>
              <div class="online_content overz" id="onlineType3" style="display:none">  
            {% if contact.hotline1 is not empty %}
                <p>
                    <img border="0" src="{{asset('/bundles/topxiaweb/img/contact/telephone.png')}}" alt="Call us" title="Call us">
                    {{contact.hotline1}}
                </p>
            {% endif %}

            {% if contact.hotline2 is not empty %}
                 <p>
                    <img border="0" src="{{asset('/bundles/topxiaweb/img/contact/telephone.png')}}" alt="Call us" title="Call us">
                    {{contact.hotline2}}
                </p>
            {% endif %}

            {% if contact.hotline3 is not empty %}
                <p>
                    <img border="0" src="{{asset('/bundles/topxiaweb/img/contact/telephone.png')}}" alt="Call us" title="Call us">
                    {{contact.hotline3}}
                </p>
            {% endif %}
          </div>
        </div>
          {% endif %}

          {% if contact.webchat is not empty %}
        <div class="online_bar mycollapse" id="onlineSort4">
              <h2><a id="4">微信</a></h2>
              <div class="online_content overz" id="onlineType4" style="display:none">  
           
               <p>
                <img border="0" src="{{asset('/bundles/topxiaweb/img/contact/webchat.png')}}" alt="微信" title="微信">
                <a href="#" class="hovera">{{contact.webchat}}</a>
               </p>
                <div class="displayimg">
                  <img  boarder="0" src="{{ asset(contact.webchatURI) }}" alt="微信二维码" title="微信二维码">
                </div>
                
          </div>
        </div>
        {% endif %}

          {% if contact.email is not empty %}
          <div class="online_bar mycollapse" id="onlineSort5">
              <h2><a id="5">电子邮件</a></h2>
              <div class="online_content overz" id="onlineType5" style="display:none">  
                <p>
                  <img border="0" src="{{asset('/bundles/topxiaweb/img/contact/email24.png')}}" alt="email" title="email">
                  <a href="mailto:{{contact.email}}">{{contact.email}}</a>
                </p>
          
          </div>
        </div>
         {% endif %}
         
        </div>
      
      <div class="side_bottom">
      </div>
    </div>
  </div>
  <div class="show_btn"><span>马上咨询</span></div>
</div>
