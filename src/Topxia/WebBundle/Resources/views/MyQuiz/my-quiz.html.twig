{% extends 'TopxiaWebBundle:MyQuiz:layout.html.twig' %}

{% block list %}
<table class="table table-striped table-hover">
	<tbody>
		{% for myTestPaperResult in myTestPaperResults %}
			{% set myTestPaper = myTestPapers[myTestPaperResult.testId] %}
			{% set course = courses[myTestPaper.targetId] %}
			{% if myTestPaperResult.status != 'doing' %}
				{% include 'TopxiaWebBundle:MyQuiz:my-quiz-tr.html.twig' %}
			{% endif %}
		{% endfor %}
	</tbody>
</table>
{{ web_macro.paginator(paginator) }}
{% endblock %}