<table class="table table-condensed table-bordered table-striped table-hover">
        <thead>
          <tr>
            <th width="35%"></th>
            <th width="20%">今日</th>
            <th width="20%" >昨日</th>
            <th width="40%">历史</th>
          </tr>
        </thead>
        <tbody>
			<tr>
				<td>新注册用户数</td>
				<td>{{todayRegisterNum}}</td>
				<td>{{yesterdayRegisterNum}}</td>
				<td><a href="{{path('admin_operation_analysis_register',{tab:"trend"})}}">趋势</a> | <a href="{{path('admin_operation_analysis_register',{tab:"detail"})}}">详情</a></td>
			</tr>

			<tr>
				<td>用户登录数<small>（不含重复登录)</small></td>
				<td>{{todayLoginNum}}</td>
				<td>{{yesterdayLoginNum}}</td>
				<td><a href="{{path('admin_operation_analysis_login',{tab:"trend"})}}">趋势</a> | <a href="{{path('admin_operation_analysis_login',{tab:"detail"})}}">详情</a></td>
			</tr>

			<tr>
				<td>新增课程数</td>
				<td>{{todayCourseNum}}</td>
				<td>{{yesterdayCourseNum}}</td>
				<td><a href="{{path('admin_operation_analysis_course',{tab:"trend"})}}">趋势</a> | <a href="{{path('admin_operation_analysis_course',{tab:"detail"})}}">详情</a></td>
			</tr>

			<tr>
				<td>新增课时数</td>
				<td>{{todayLessonNum}}</td>
				<td>{{yesterdayLessonNum}}</td>
				<td><a href="{{path('admin_operation_analysis_lesson',{tab:"trend"})}}">趋势</a> | <a href="{{path('admin_operation_analysis_lesson',{tab:"detail"})}}">详情</a></td>
			</tr>

			<tr>
				<td>加入学习数</td>
				<td>{{todayJoinLessonNum}}</td>
				<td>{{yesterdayJoinLessonNum}}</td>
				<td><a href="{{path('admin_operation_analysis_lesson_join',{tab:"trend"})}}">趋势</a> | <a href="{{path('admin_operation_analysis_lesson_join',{tab:"detail"})}}">详情</a></td>
			</tr>

			<tr>
				<td>购买课程数</td>
				<td>{{todayBuyLessonNum}}</td>
				<td>{{yesterdayBuyLessonNum}}</td>
				<td><a href="{{path('admin_operation_analysis_lesson_paid',{tab:"trend"})}}">趋势</a> | <a href="{{path('admin_operation_analysis_lesson_paid',{tab:"detail"})}}">详情</a></td>
			</tr>

			<tr>
				<td>完成课时学习数</td>
				<td>{{todayFinishedLessonNum}}</td>
				<td>{{yesterdayFinishedLessonNum}}</td>
				<td><a href="{{path('admin_operation_analysis_lesson_finished',{tab:"trend"})}}">趋势</a> | <a href="{{path('admin_operation_analysis_lesson_finished',{tab:"detail"})}}">详情</a></td>
			</tr>

			<tr>
				<td>视频观看数<small>（* 含重复打开）</small></td>
				<td>{{todayAllVideoViewedNum}}</td>
				<td>{{yesterdayAllVideoViewedNum}}</td>
				<td><a href="{{path('admin_operation_analysis_video_viewed',{tab:"trend"})}}">趋势</a> | <a href="{{path('admin_operation_analysis_video_viewed',{tab:"detail"})}}">详情</a></td>
			</tr>

			<tr>
				<td>云视频观看数</td>
				<td>{{todayCloudVideoViewedNum}}</td>
				<td>{{yesterdayCloudVideoViewedNum}}</td>
				<td>
				{% if keyCheckResult.status|default(null) == 'error' %}
					<a href="#">还未开通云视频，点击开通</a>
				{% endif %}

				{% if keyCheckResult.error|default(null) %}
					AccessKey或者SecretKey设置不正确
				{% endif %}

				<a href="{{path('admin_operation_analysis_video_cloud_viewed',{tab:"trend"})}}">趋势</a> | <a href="{{path('admin_operation_analysis_video_cloud_viewed',{tab:"detail"})}}">详情</a></td>
			</tr>

			<tr>
				<td>本地视频观看数</td>
				<td>{{todayLocalVideoViewedNum}}</td>
				<td>{{yesterdayLocalVideoViewedNum}}</td>
				<td><a href="{{path('admin_operation_analysis_video_local_viewed',{tab:"trend"})}}">趋势</a> | <a href="{{path('admin_operation_analysis_video_local_viewed',{tab:"detail"})}}">详情</a></td>
			</tr>

			<tr>
				<td>网络视频观看数</td>
				<td>{{todayNetVideoViewedNum}}</td>
				<td>{{yesterdayNetVideoViewedNum}}</td>
				<td><a href="{{path('admin_operation_analysis_video_net_viewed',{tab:"trend"})}}">趋势</a> | <a href="{{path('admin_operation_analysis_video_net_viewed',{tab:"detail"})}}">详情</a></td>
			</tr>

			<tr>
				<td>营收额</td>
				<td>{{todayIncome}}</td>
				<td>{{yesterdayIncome}}</td>
				<td><a href="{{path('admin_operation_analysis_income',{tab:"trend"})}}">趋势</a> | <a href="{{path('admin_operation_analysis_income',{tab:"detail"})}}">详情</a></td>
			</tr>

			<tr>
				<td>课程营收额</td>
				<td>{{todayCourseIncome}}</td>
				<td>{{yesterdayCourseIncome}}</td>
				<td><a href="{{path('admin_operation_analysis_course_income',{tab:"trend"})}}">趋势</a> | <a href="{{path('admin_operation_analysis_course_income',{tab:"detail"})}}">详情</a></td>
			</tr>

			<tr>
				<td>退出学习数</td>
				<td>{{todayExitLessonNum}}</td>
				<td>{{yesterdayExitLessonNum}}</td>
				<td><a href="{{path('admin_operation_analysis_lesson_exit',{tab:"trend"})}}">趋势</a> | <a href="{{path('admin_operation_analysis_lesson_exit',{tab:"detail"})}}">详情</a></td>
			</tr>


        </tbody>
</table>
