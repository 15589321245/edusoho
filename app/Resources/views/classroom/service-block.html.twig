{% do load_script('topxiawebbundle/controller/classroom/service-block') %}
{% include 'seajs_loader_compatible.html.twig' with {'topxiawebbundle':true} %}

<ul class="list-unstyled clearfix">
  {% set defaultServices = dict('classroomService')|keys %}
  {% for service in services %}
    {% include 'classroom/part/service/' ~ service ~ '.html.twig' with {active:true} %}
  {% endfor %}
  
  {% for service in defaultServices if service not in services %}
    {% include 'classroom/part/service/' ~ service ~ '.html.twig' with {active:false} %}
  {% endfor %}
</ul>