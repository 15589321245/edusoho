{% if testPaperResult.status in ['doing', 'paused'] %}

<tr>
	<td>
		<a href="">{{ question.stem }}</a> 
		<br>
		<small> 试卷没做完不可以偷看解答哦~ </small>
	</td>
	<td width="20%">
		<a href="" class="btn btn-info pull-right disabled">查看题目</a>
	</td>
</tr>

{% else %}

<tr>
	<td>
		<a href="{{ path('course_manage_test_results', {id:testPaperResult.id}) }}">{{ question.stem }}</a> 
		<br>
		<small>来自试卷《{{ testPaper.name }}》，{{ testPaperResult.beginTime|date("Y-m-d H:i:s") }}，得分{{ testPaperResult.score }}分，共{{ testPaper.itemCount }}题，做对{{ testPaperResult.rightItemCount }}题。</small>
	</td>
	<td width="20%">
		<a href="{{ path('course_manage_test_results', {id:testPaperResult.id}) }}" class="btn btn-info pull-right">查看题目</a>
	</td>
</tr>

{% endif %}