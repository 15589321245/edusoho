{% if inspectList %}
  <div class="alert alert-warning" role="alert">
    {% for inspectItem in inspectList %}
      <span>{{inspectItem.value.errorMessage}}</span>
      <a href='{{inspectItem.value.settingUrl}}'>{{'admin.homepage.alert.setting_domain'|trans}}</a>
    {% endfor %}
  </div>
{% endif %}