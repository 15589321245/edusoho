'use strict';

var _help = require('app/js/course-manage/help');

(0, _help.publishCourse)();
startIntro();

function startIntro() {
  var intro = introJs();
  intro.setOptions({
    steps: [{
      intro: "课程管理功能现已全新升级!"
    }, {}],
    skipLabel: '我知道了',
    nextTooltipButton: '下一步',
    prevTooltipButton: '上一步'
  });
  console.log(intro);
  intro.start();
}