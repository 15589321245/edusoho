{% extends 'TopxiaAdminBundle:Analysis:layout.html.twig' %}

{% block title %}业务订单统计 - {{ parent() }}{% endblock %}

{% set menu = 'businessState' %}



{% block main %}
	<div class="page-header">
   
    <h1>业务订单统计</h1>
  </div>

  <form id="message-search-form" class="form-inline well well-sm" action="" method="get" novalidate>
   
  </form>

  <table class="table table-striped table-hover" id="activity-table">
    <thead>
    <tr>
      <th>日期</th>
      <th>产品类型</th>  
      <th>产品Id</th>  
      <th>产品名称</th>
      
      <th>订单数</th>
      <th>收费定单数</th>
      <th>免费定单数</th>
      <th>Pv</th>      
      <th>Uv</th>
    
    </tr>
    </thead>
    <tbody>
      {% for businessState in businessStates %}
     
      
        {% include 'TopxiaAdminBundle:Analysis:business-state-tr.html.twig' %}
      {% else %}
        <tr><td colspan="6"><div class="empty">无课程记录</div></td></tr>
      {% endfor %}
    </tbody>
  </table>

  {{ web_macro.paginator(paginator) }}
{% endblock %}