{% extends 'TopxiaWebBundle:MySale:layout.html.twig' %}

{% block title %}收入明细 - {{ parent() }}{% endblock %}
{% set side_nav = 'commission-list' %}

{% block main %}
<div class="panel panel-default panel-col">
  <div class="panel-heading">
     佣金收入明细
  </div>
  <div class="panel-body">

  <table class="table table-striped table-hover" id="commission-table">
    <thead>
    <tr>

      <th>日期</th>
      <th>佣金(元)</th>
      <th>状态</th>   
      <th>所属定单</th>
      <th>所属推广</th>  
      <th>购买客户</th>
      
    </tr>
    </thead>
    <tbody>
      {% for commission in commissions %}
        <tr>
         
        </tr>
      {% else %}
        <tr><td colspan="7">无数据</td></tr>
      {% endfor %}
    </tbody>
  </table>

  {{ web_macro.paginator(paginator) }} 

  </div>
</div>

{% endblock %}