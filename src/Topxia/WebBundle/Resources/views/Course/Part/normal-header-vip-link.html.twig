{% if userVipStatus %}
		{% if userVipStatus == 'not_login' %}
			<a href="{{ path('login') }}" class="color-w" >会员</a>
		{% elseif userVipStatus in ['not_member', 'member_expired'] %}
			<a href="{{ path('vip_buy') }}" class="color-w">会员</a>  
		{% elseif userVipStatus == 'level_low' %}
			<a href="{{ path('vip_upgrade', {level:classroomMemberLevel.id}) }}" class="color-w" >会员</a>
		{% elseif userVipStatus == 'ok' %}
			<a href="javascript:;" class="color-w">会员</a>
		{% endif %}
{% endif %}