{% for path in script()  %}
  {% if path starts with 'http://' %}
    {% if 'js-sdk/uploader' in path %}
      <script src="{{ path ~ '?' ~ random(date().timestamp / 1000) }}"></script>
    {% else %}
      <script src="{{ path ~ '?' ~ asset_version('')}}"></script>
    {% endif %}
  {% else %}
    <script src="{{ asset('static-dist/' ~ path) }}"></script>
  {% endif %}
{% endfor %}