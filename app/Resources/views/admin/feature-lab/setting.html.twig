{% extends 'admin/layout.html.twig' %}

{% set menu = 'admin_cloud_feature_lab' %}

{% set script_controller = 'feature_lab/setting' %}

{% block main %}

	<div class="clearfix lab-setting-item">
    <div class="pull-left">
      <h4>人脸识别</h4>
      <div class="text-sm text-muted">开启后，你可以使用基于人脸识别服务的一系列应用，比如人脸识别登录等。</div>
    </div>
    <div class="text-right admin-common-switch mtm">
      <label class="switch switch-lg switch-green js-switch {% if setting('feature_lab.face_identify') == 1 %}checked{% endif %}">
        <input type="checkbox" name="face_identify" value="{{ setting('feature_lab.face_identify') }}" >
      </label>
    </div>
  </div

{% endblock %}