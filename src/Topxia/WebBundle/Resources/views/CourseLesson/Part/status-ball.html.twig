 {% if learnStatuses[item.id]|default(null) %}
  {% if learnStatuses[item.id] == 'finished' %}
    <i class="es-icon es-icon-done1 color-p status-icon"></i>
  {% else %}
    <i class="es-icon es-icon-doing color-p status-icon"></i>
  {% endif %}
{% else %}
  <i class="es-icon es-icon-undone status-icon"></i>
{% endif %}