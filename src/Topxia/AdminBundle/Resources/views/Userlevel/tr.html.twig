<tr id="userlevel-{{ userlevel.id }}" class="item-userlevel">
  <td>
    <span class="glyphicon glyphicon-move" style="cursor:move"></span>
  </td>
  <td width="30%">
    <img src="{{ userlevel.icon }}">&nbsp;{{ userlevel.name}}
  </td>
  <td>
    {% if userlevel.monthPrice == 0 %}-{% else %}{{ userlevel.monthPrice }}{% endif %}
  </td>
  <td>
    {% if userlevel.yearPrice == 0 %}-{% else %}{{ userlevel.yearPrice }}{% endif %}
  </td>
  <td>
    123
  </td>
  <td>
    {% if userlevel.enabled %}
      <span class="text-success">开启</span>
      <br>
    {% else %}
      <span class="text-muted">关闭</span>
    {% endif %}  
  </td>
  <td>
    <button class="btn btn-default btn-sm" data-url="{{path('admin_user_level_update',{id:userlevel.id}) }}" data-toggle="modal" data-target="#modal">编辑</button>
    <button href="javascript:" class="btn btn-default btn-sm delete-userlevel" data-url="{{path('admin_user_level_delete',{id:userlevel.id}) }}" >删除</button>
  </td>
</tr>
