<table class="table table-striped table-hover" id="conversion-order-list-table" style="word-break:break-all;">
  <thead>
    <tr>
      <th width="35%">{{ '促成了购买以下内容'|trans }}</th>
      <th>{{ '类型'|trans }}</th>
      <th>{{ '购买数'|trans }}</th>
      <th>{{ '价格'|trans }}</th>
    </tr>
  </thead>
  <tbody>
    {% for orderLog in orderLogs %}
      {% set target = orderLog.target|default(null) %}
      <td>
        {% if target %}
          {{ target.title }}
        {% else %}
          -
        {% endif %}
      </td>
      <td>{{ orderLog.targetType }}</td>
      <td>{{ orderLog.buyNum }}</td>
      <td>
        {% if orderLog.targetType == 'vip'%} - {% else %}{{ target.price }}{% endif %}
      </td>
    {% else %}
      <tr><td colspan="20"><div class="empty">{{ '暂无记录'|trans }}</div></td></tr>
    {% endfor %}
  </tbody>
</table>
