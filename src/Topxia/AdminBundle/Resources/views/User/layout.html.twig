{% extends 'TopxiaAdminBundle::layout.html.twig' %}
{% set side_menus = menus("admin",'admin_user') %}
{% set nav = 'admin_user' %}

{% block content %}
  <div class="col-md-2">
    {% block sidebar %}

      <div class="panel panel-default">

          {% include 'TopxiaAdminBundle::side.html.twig' with {menus:side_menus,nav:menu} %}
      </div>
      
    {% endblock %}
  </div>
  <div class="col-md-10">
      <div class="page-header"><h1>{{ admin_macro.title(side_menus,menu) }}</h1></div>
      {% block main %}{% endblock %}
  </div>
{% endblock %}
{% block page_script %} app.load('user') {% endblock %}