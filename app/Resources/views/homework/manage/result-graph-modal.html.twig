{% extends 'bootstrap-modal-layout.html.twig' %}
{% set modal_class= "modal-lg" %}

{% do script(['libs/echarts.js','app/js/testpaper-manage/result/index.js']) %}

{% block title %}
  {% if task.isOptional %}
    {{ 'course.testpaper_check.optional_task_title'|trans({'%title%':task.title}) }}
  {% else %}
    {{ 'course.catalogue.task_status.task'|trans({'%taskNumber%':task.number, '%taskTitle%':task.title}) }}
  {% endif %}
{% endblock %}

{% block body %}
<div>合格率：{{ analysis.passPercent|default(0) }}%</div>
<div class="col-md-12">  
  <div id="line-data" style="width: 100%;height:400px;"></div>
  <input id="data"  type="hidden" value='{% if data %}{{data}}{% endif %}'>  
</div>
{% endblock %}

{% block footer %}{% endblock %}