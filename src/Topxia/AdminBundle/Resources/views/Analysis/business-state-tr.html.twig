{% import "TopxiaAdminBundle::macro.html.twig" as admin_macro %}
<tr >
  <td>{{ businessState.date }}</td>
  <td>{{ businessState.prodType }}</td>
   <td>{{ businessState.prodId }}</td>
  <td>{{ businessState.prodName }}</td>
 
  <td>{{ businessState.orders }}
    <br>

    {% if businessState.dayGuest >0 %}
    <span class="text-color-red text-sm">  {{ (businessState.orders/businessState.dayGuest)*100  }}% </span>
    {% endif %}

  </td>
  <td>{{ businessState.feeOrders }}
   <br>

    {% if businessState.dayGuest >0 %}
    <span class="text-color-red text-sm">  {{ (businessState.feeOrders/businessState.dayGuest)*100  }}% </span>
    {% endif %}
  </td> 
  <td>{{ businessState.freeOrders }} <br>

    {% if businessState.dayGuest >0 %}
    <span class="text-color-red text-sm">  {{ (businessState.freeOrders/businessState.dayGuest)*100  }}% </span>
    {% endif %}
  </td> 

  <td>{{ businessState.dayGuest }}</td>
  <td>{{ businessState.dayGuestVisit }}</td>

  <td>{{ businessState.dayNewGuest }}</td>
  <td>{{ businessState.dayNewGuestVisit }}</td>

    <td>{{ businessState.dayOldGuest }}</td>
  <td>{{ businessState.dayOldGuestVisit }}</td>

    <td>{{ businessState.dayUser }}</td>
  <td>{{ businessState.dayUserVisit }}</td>
  
</tr>