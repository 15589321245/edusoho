<ul id="sortable-list" class="task-manage-list sortable-list"
    data-sort-url='{{ path("course_manage_items_sort",{courseId: course.id}) }}'>
  {% for key, item in items %}
    {% if item.itemType == 'task' %}
      {% include 'WebBundle:TaskManage:list-item-byOrder.html.twig' with {task:item} %}
    {% elseif item.itemType == 'chapter' %}
      {% include 'WebBundle:CourseChapterManage:list-item.html.twig' with {chapter:item, course:course} %}
    {% endif %}
  {% endfor %}
</ul>