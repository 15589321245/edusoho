      <table id="user-table" class="table table-striped table-hover" data-search-form="#user-search-form">
          <thead>
            <tr>
              <th>{{'用户名'|trans}}</th>
              <th>{{'注册邮箱'|trans}}</th>
              <th>{{'注册来源'|trans}}</th>
              <th>{{'IP地址'|trans}}</th>
              <th>{{'注册时间'|trans}}</th>
            </tr>
          </thead>
          <tbody>
            {% if registerDetail %}
            {% for data in registerDetail %}
              <tr>
                <td>{{data.nickname}}</td>
                <td>{{data.email}}</td>
                <td>{% if data.type=="default" %}{{'网站注册'|trans}}
                  {% elseif data.type=="weibo" %}{{'新浪微博'|trans}}
                  {% elseif data.type=="qq" %}QQ
                  {% elseif data.type=="renren" %}{{'人人'|trans}}
                  {% elseif data.type=="discuz" %}discuz
                  {% elseif data.type=="phpwind" %}phpwind
                  {% else %}{{'其他'|trans}}
                  {% endif %}</td>
                <td>{{data.createdIp}}</td>
                <td>{{data.createdTime|date("Y-m-d H:i:s")}}</td>
              </tr>
            {% endfor %}
            {% endif %}
          </tbody>
      </table>    
      {{ web_macro.paginator(paginator) }}